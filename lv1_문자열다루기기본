#include <string>
#include <vector>
#include <iostream>
using namespace std;

bool solution(string s) {
    bool answer = true;
    
    if(s.size() == 4 || s.size() == 6){
        for(int index = 0; index < s.size(); index++){
            if(s[index] < '0' || s[index] > '9')
                return false;
        }
    }
    else
        return false;
    
    return answer;
}
